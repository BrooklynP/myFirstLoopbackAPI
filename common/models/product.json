{
  "name": "product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
  "Name": {
    "type": "string",
    "required": false
    },
  "defaultPrice": {
    "type": "number",
    "required": false
    },
  "ActiveFlag": {
    "type": "boolean",
    "required": false
    },
  "CreatedBy": {
    "type": "string",
    "required": false
    },
  "CreatedOn": {
    "type": "date",
    "required": false
    },
  "ModifiedBy": {
    "type": "string",
    "required": false
    },
  "ModifiedOn": {
    "type": "date",
    "required": false
    }
  },
  "validations": [],
  "relations": {
    "orderlines":{
      "type":"hasMany",
      "model":"orderLine",
      "foreignKey":"productID",
      "options":{
        "nestRemoting":true
      }
    }

  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
